{
    "tmpdir": "/tmp",
    "music": {
        "rootdir": "/mnt/Ishamedia",
        "types": "mp3,wav",
        "autoplay": "false"
    },
    "playlist": {
        "rootdir": "/mnt/Ishamedia",
        "types": "imc,json"
    },
    "video": {
        "rootdir": "/mnt/Ishamedia",
        "types": "mp4",
        "autoplay": "false"
    },
    "settings": {
        "osdTime": 10,
        "runtimeInterval": 1,
        "screensaverTime": 10,
        "hdmiBoost": 4,
        "audioSource": 0,
        "hdmiResolution": 0,
        "volIncVal": 5,
        "player": "vlc"
    },
    "httpServerIp": {
        "ip": "127.0.0.1",
        "port": "11111"
    },
    "mpv": {
        "parameters": [
            "mpv",
            "--geometry={width}",
            "--start=+{start}",
            "--no-border",
            "--no-input-default-bindings",
            "{path}",
            "--really-quiet",
            "--no-osc",
            "--no-input-terminal",
            "--input-ipc-server={socket}"
        ]
    },
    "amixer": {
        "get": [
            "amixer",
            "sget",
            "Master"
        ],
        "set": [
            "amixer",
            "sset",
            "Master"
        ]
    },
    "ipcOsdPort": 40001,
    "ipcWmPort": 40002,
    "hdmiCfgPath": "/tmp/imc_hdmi.txt",
    "scancodes": {
        "1": "esc",
        "14": "back",
        "25": "power",
        "27": "volumeup",
        "28": "enter",
        "35": "home",
        "50": "mute",
        "53": "volumedown",
        "88": "home",
        "103": "up",
        "104": "pgup",
        "105": "left",
        "106": "right",
        "108": "down",
        "109": "pgdown",
        "113": "mute",
        "114": "volumedown",
        "115": "volumeup",
        "116": "power",
        "158": "back",
        "164": "playpause",
        "172": "home",
        "272": "enter"
    }
}